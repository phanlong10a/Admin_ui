package com.globits.covid19.test.dto;

import java.util.List;
import java.util.UUID;

public class UserInfoDto {

	private Integer orgType;
	private UserOrganizationDto userOrganization;
	private boolean isAdmin = false;
	private boolean isUser = false;
	private List<UUID> listChildHealthOrganizationId;
	
	public Integer getOrgType() {
		return orgType;
	}
	public void setOrgType(Integer orgType) {
		this.orgType = orgType;
	}
	public UserOrganizationDto getUserOrganization() {
		return userOrganization;
	}
	public void setUserOrganization(UserOrganizationDto userOrganization) {
		this.userOrganization = userOrganization;
	}
	public boolean isAdmin() {
		return isAdmin;
	}
	public void setAdmin(boolean isAdmin) {
		this.isAdmin = isAdmin;
	}
	public boolean isUser() {
		return isUser;
	}
	public void setUser(boolean isUser) {
		this.isUser = isUser;
	}
	
	public List<UUID> getListChildHealthOrganizationId() {
		return listChildHealthOrganizationId;
	}
	public void setListChildHealthOrganizationId(List<UUID> listChildHealthOrganizationId) {
		this.listChildHealthOrganizationId = listChildHealthOrganizationId;
	}
	public UserInfoDto() {
		super();
	}
	
}
