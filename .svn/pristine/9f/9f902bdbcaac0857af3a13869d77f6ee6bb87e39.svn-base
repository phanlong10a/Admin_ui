import axios from "axios";
import ConstantList from "../../appConfig";
const API_PATH_ROLE = ConstantList.API_ENPOINT + "/api/report/";
const API_PATH_EXCEL = ConstantList.API_ENPOINT + "/api/downloadExcel/";

export const sampleReportBySuspectedLevel = (dto) => {
  var params = "sampleReportBySuspectedLevel";
  var url = API_PATH_ROLE + params;
  return axios.post(url,dto);
};

export const exportExcelSampleReportBySuspectedLevel = (dto) => {
  var params = "exportExcelSampleReportBySuspectedLevel";
  var urll = API_PATH_EXCEL + params;
  return axios({
    url: urll,
    method: "POST",
    responseType: "blob", // important
    data: dto,
  })
};
